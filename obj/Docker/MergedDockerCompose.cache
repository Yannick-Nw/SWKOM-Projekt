name: dockercompose15214673535529678421
services:
  postgres:
    environment:
      POSTGRES_DB: paperless
      POSTGRES_PASSWORD: paperless
      POSTGRES_USER: paperless
    image: postgres:13
    networks:
      backend: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: postgres_data
        target: /var/lib/postgresql/data
        volume: {}
  postgres-adminer:
    image: adminer
    networks:
      backend: null
    ports:
      - mode: ingress
        target: 8080
        published: "8082"
        protocol: tcp
    restart: always
  webapi:
    build:
      context: C:\Users\dilan.boskan.GOEGIT02\source\repos\Yannick-Nw\SWKOM-Projekt\WebApi
      dockerfile: Dockerfile
    depends_on:
      postgres:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    networks:
      backend: null
      frontend: null
    ports:
      - mode: ingress
        target: 8080
        published: "8080"
        protocol: tcp
    restart: always
  webapp:
    build:
      context: C:\Users\dilan.boskan.GOEGIT02\source\repos\Yannick-Nw\SWKOM-Projekt\WebApp
      dockerfile: Dockerfile
    depends_on:
      webapi:
        condition: service_started
        required: true
    networks:
      frontend: null
    ports:
      - mode: ingress
        target: 80
        published: "80"
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: C:\Users\dilan.boskan.GOEGIT02\source\repos\Yannick-Nw\SWKOM-Projekt\WebApp\nginx.conf
        target: /etc/nginx/nginx.conf
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\dilan.boskan.GOEGIT02\source\repos\Yannick-Nw\SWKOM-Projekt\WebApp\html
        target: /app/wwwroot
        bind:
          create_host_path: true
networks:
  backend:
    name: dockercompose15214673535529678421_backend
    driver: bridge
  frontend:
    name: dockercompose15214673535529678421_frontend
    driver: bridge
volumes:
  postgres_data:
    name: dockercompose15214673535529678421_postgres_data